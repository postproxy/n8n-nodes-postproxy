{
  "name": "n8n-nodes-postproxy",
  "version": "0.1.4",
  "description": "n8n community node for PostProxy API - unified API for publishing and scheduling posts across multiple social media platforms",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "postproxy",
    "social-media",
    "scheduling",
    "twitter",
    "facebook",
    "instagram",
    "automation"
  ],
  "author": {
    "name": "Dmitry Sereda",
    "email": "contact@postproxy.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/postproxy/n8n-nodes-postproxy.git"
  },
  "bugs": {
    "url": "https://github.com/postproxy/n8n-nodes-postproxy/issues"
  },
  "homepage": "https://www.postproxy.dev/",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/**/*.svg dist",
    "prepare": "npm run build",
    "dev": "npm run build && npm run watch",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lintfix": "eslint src --ext .ts --fix",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "n8n": {
    "nodes": [
      "dist/nodes/PostProxy/PostProxy.node.js"
    ],
    "credentials": [
      "dist/credentials/PostProxyApi.credentials.js"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unused-vars": "warn"
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "n8n-core": "^2.2.3",
    "n8n-workflow": "^2.2.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
